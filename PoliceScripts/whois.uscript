// WORKS ONLY ON BARRICADES
command whois(){ // Command = /whois
    permission = "whois"; // Defines permisission needed if not admin
    execute() {
    barricade = player.look.getBarricade(); // Checks if looking at a barricade
    Player = toPlayer(barricade.owner); // Get barricade Owner
    player.message("Owner: "+Player.name+"("+barricade.owner+") "+"Group ID: "+barricade.group+" Barricade: "+barricade.itemType); // Outputs and gets data to player who ran the command
    }
}